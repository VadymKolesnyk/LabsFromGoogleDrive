} def
/resistor
{
    gsave translate rotate
        newpath
            0   -2.5    moveto
            0   5       rlineto
            10  0       rlineto
            0   -5      rlineto
        closepath
        stroke
    grestore
} def
/capacitor
{
    gsave translate rotate
        newpath
            0   0       moveto
            2.5 0       rlineto
            0   5       rmoveto
            0   -10     rlineto
            5   0       rmoveto
            0   10      rlineto
            0   -5      rmoveto
            2.5 0       rlineto
        closepath
        stroke  
    grestore 
} def
/inductor
{
    /radius {10 6 div} def 
    gsave translate rotate
        newpath
            radius 0 radius 180 0 arc 
            3 radius mul 0 radius 180 0 arc 
            5 radius mul 0 radius 180 0 arc 
        stroke
    grestore
} def
/voltagesource
{
    gsave translate rotate
        newpath
            5 0 5 0 360 arc 
            0 0 lineto
            7.5 1.5 moveto
            10 0 lineto
            7.5 -1.5 moveto
            10 0 lineto
        stroke
    grestore
} def
/diode
{
    gsave translate rotate
        newpath
            2.5 2.88675 moveto
            2.5 -2.88675 lineto
            7.5 0 lineto
        closepath
        stroke
        newpath
            7.5 2.88675 moveto
            7.5 -2.88675 lineto
            0 0 moveto
            2.5 0 lineto
            7.5 0 moveto
            10 0 lineto 
        stroke
    grestore
} def
/transistor
{
    gsave translate rotate
        newpath
            5 0 5 -90 270 arc 
            2.5 0 lineto
            4.75 4.5 lineto
            2.5 4.330127 moveto
            2.5 -4.330127 lineto
            0 0 moveto
            2.5 0 lineto
        stroke
        gsave
            5 5 translate
            63.43495 rotate
            2 2 scale
            newpath
                0 0 moveto
                -1 0.57735 lineto
                -1 -0.57735 lineto
                closepath
            fill
        grestore  
    grestore  
} def
/currentsource
{
    gsave translate rotate 
        newpath
            5 0 5 0 360 arc
            2.5 2.5 moveto
            5 0 lineto
            2.5 -2.5 lineto
            5 2.5 moveto
            7.5 0 lineto
            5 -2.5 lineto
        stroke
    grestore
} def
/dot
{
    gsave translate
        newpath
            0 0 1 0 360 arc
        fill
    grestore
} def




